{
    "name": "tecdiary/laravel-sql-dump",
    "description": "Dump Laravel Migrations to SQL File",
    "keywords": [
        "tecdiary",
        "laravel-sql-dump"
    ],
    "homepage": "https://github.com/tecdiary/laravel-sql-dump",
    "license": "MIT",
    "authors": [
        {
            "name": "Mian Saleem",
            "email": "mian.saleem@yahoo.com",
            "homepage": "https://tecdiary.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Tecdiary\\LaravelSqlDump\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tecdiary\\LaravelSqlDump\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tecdiary\\LaravelSqlDump\\LaravelSqlDumpServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
